{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "tailwind.config.js", "pages/Contact/Contact.jsx", "pages/_app.jsx", "pages/admin/dashboard.jsx", "pages/admin/index.jsx", "pages/admin/VerifyEmail.jsx", "pages/Home/cards.jsx"],
  "exclude": ["node_modules"]
}
